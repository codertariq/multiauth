{
    "name": "tariqul/multiauth",
    "description": "This is package for Multiple Auhtenication on your laravel project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tariqul Islam",
            "email": "tariqulislamrc@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "spatie/laravel-permission": "^2.25",
        "yajra/laravel-datatables": "^1.0"
    },
    "require-dev":{
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "orchestra/testbench": "^3.8@dev",
        "orchestra/database": "^3.8@dev",
        "illuminate/support": "^5.8@dev",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "Tariqul\\Multiauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tariqul\\Multiauth\\Tests\\":"tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tariqul\\Multiauth\\MultiAuthServiceProvider"
            ]
        }
    }
}
